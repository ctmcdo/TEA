<?xml version="1.0" encoding="utf-8"?>



<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tea.cmcdona2.casper"
    android:versionCode="2"
    android:versionName="2.0">


    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/casper"
        android:label="@string/app_name"

        android:theme="@style/AppTheme">
        <activity
            android:screenOrientation="portrait"
            android:name="com.tea.cmcdona2.casper.Splash.SplashActivity"
            android:label="@string/app_name"></activity>

        <activity
            android:name="com.tea.cmcdona2.casper.Socs.SocsActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">

            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>


        <activity android:name="com.tea.cmcdona2.casper.Ents.EntsActivity"
            android:screenOrientation="portrait"
            android:label=""
            />

   
        <activity
            android:name="com.tea.cmcdona2.casper.ParticularEnt.ParticularEntActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.tea.cmcdona2.casper.ParticularSoc.ParticularSocActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
